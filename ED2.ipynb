        "      return min_index\n",
        " \n",
        "    def Dijkstra(self, value):\n",
        "      distancia = [sys.maxsize] * self.Vertices\n",
        "      distancia[value] = 0\n",
        "      swDij = [False] * self.Vertices\n",
        "      for cout in range(self.Vertices):\n",
        "        a = self.minDistancia(distancia, swDij)\n",
        "        swDij[a] = True\n",
        "        for v in range(self.Vertices):\n",
