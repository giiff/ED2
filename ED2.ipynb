        "\n",
        "    def BellmanFord(self, value):\n",
        "      distancia = [float(\"Inf\")] * self.Vertices\n",
        "      distancia[value] = 0\n",
        "      for _ in range(self.Vertices - 1):\n",
        "        for a, b, c in self.Grafo:\n",
        "          if distancia[a] != float(\"Inf\") and distancia[a] + c < distancia[b]:\n",
        "            distancia[b] = distancia[a] + c\n",
        "      for a, b, c in self.Grafo:\n",
